/* pages/paihang/paihang.wxss */
.diji{
   width: 100%;
   height: 50rpx;
   display: flex;
   flex-direction: row;
   justify-content: center;
   align-items: center;
   font-size: 30rpx;
}
.jilu{
  width: 90%;
  height: 150rpx;
  box-shadow: #dddee1 10rpx 10rpx 10rpx 10rpx;
  border-radius: 30rpx;
  margin-left: 5%;
  margin-top: 20rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-items: center;
}
.jilu_left{
  width: 45%;
  height: 90%;
  
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-items: center;
}
.jilu_right{
  width: 45%;
  height: 90%;
  
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-items: center;
}
.shuju{
  width: 50%;
  height: 90%;
  
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
  font-size: 30rpx;
}
.shuju_img{
  width: 50rpx;
  height: 50rpx;
  
}
.left_round{
  width: 40%;
  height: 90%;
  border-radius: 50%;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  overflow: hidden;
  background-color: white;
  border: #999999 solid 3rpx;
}
.left_round image{
    
    width: 60%;
    height: 60%;
    
}
.round{
  width: 40%;
  height: 90%;
  border-radius: 50%;
  
  overflow: hidden;
}
.round image{
  width: 100%;
  height: 100%;
}
.nickname{
  font-size: 30rpx;
  
  width: 50%;
  height: 90%;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  overflow: hidden;
  flex-wrap: nowrap;
}
